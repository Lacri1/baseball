<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baseball.game.mapper.PitcherMapper">
	<select id="findByTeam" resultType="com.baseball.game.dto.Pitcher">
		select Player_Name,Player_Team,Base_On_Balls,Strike_Out,WHIP,Hit_By_Pitch,Pitchers_Batters_Faced from kbo_pitcher_stats_2024 where Team=#{team}
	</select>
	<select id="findByNames" resultType="List">
		select Name from kbo_pitcher_stats_2024 where Team=#{team} and Innings_Pitched >100
	</select>
</mapper>